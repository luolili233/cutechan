{
  "files": [
    "app.ts"
  ],
  "include": [
    "node_modules/kpopnet/api"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "lib": [
      "DOM",
      "ES6",
      "DOM.Iterable",
      "ScriptHost",
      "ES2017.object"
    ],
    "downlevelIteration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "module": "AMD",
    "target": "ES6",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "allowJs": true,
    "pretty": true,
    "noEmitOnError": true,
    "preserveWatchOutput": true
  }
}